    local args = {"RolePlayBio", value}
    local remote = game:GetService("ReplicatedStorage").RE:FindFirstChild("1RPNam1eTex1t")
    if remote then
        remote:FireServer(unpack(args))
    end
end)

-- Rainbow Bio Button
-- متغير لتتبع حالة تشغيل الـ Rainbow Bio
local rainbowBioRunning = false

createToggle(AvatarsTab, "تلوين البايو", false, function(state)
    if state then
        if rainbowBioRunning then return end
        rainbowBioRunning = true
        task.spawn(function()
            while rainbowBioRunning do
                local hue = (tick() % 10) / 10
                local color = Color3.fromHSV(hue, 1, 1)
                local args = {"PickingRPBioColor", color}
                local remote = game:GetService("ReplicatedStorage"):WaitForChild("RE"):FindFirstChild("1RPNam1eColo1r")
                if remote then
                    remote:FireServer(unpack(args))
                end
                task.wait(0.15)
            end
        end)
    else
        rainbowBioRunning = false
    end
end)
-- قسم: نسخ سكنات اللاعبين FE
createSection(AvatarsTab, "نسخ سكنات اللاعبين FE")

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local LocalPlayer = Players.LocalPlayer

local selectedPlayerName = ""
local copyMode = "Brookhaven" -- "Brookhaven" = في ماب البيوت | "Real" = اللسكن الحقيقي

-- وظائف المساعدة
local function WearAsset(id)
	pcall(function()
		ReplicatedStorage.Remotes.Wear:InvokeServer(tonumber(id))
	end)
end

local function ResetBody()
	pcall(function()
		ReplicatedStorage.Remotes.ChangeCharacterBody:InvokeServer({0, 0, 0, 0, 0, 0}, "AllBlocky")
	end)
end

local function CopyAccessories(desc)
	for _, accessory in ipairs(desc:GetAccessories(true)) do
		if accessory.AssetId then
			WearAsset(accessory.AssetId)
			task.wait(0.1)
		end
	end
end

local function CopyBody(desc)
	local parts = {
		tonumber(desc.Torso),
		tonumber(desc.RightArm),
		tonumber(desc.LeftArm),
		tonumber(desc.RightLeg),
		tonumber(desc.LeftLeg),
		tonumber(desc.Head),
	}
	ReplicatedStorage.Remotes.ChangeCharacterBody:InvokeServer(parts, "ShnmaxHub")
end

local function CopyClothing(desc)
	for _, asset in ipairs({desc.Shirt, desc.Pants, desc.GraphicTShirt, desc.Face}) do
		if tonumber(asset) and asset ~= 0 then
			WearAsset(asset)
			task.wait(0.1)
		end
	end
end

local function CopyAnimations(desc)
	for _, anim in ipairs({
		desc.IdleAnimation,
		desc.WalkAnimation,
		desc.RunAnimation,
		desc.JumpAnimation,
		desc.FallAnimation,
		desc.SwimAnimation
	}) do
		if tonumber(anim) and anim ~= 0 then
			WearAsset(anim)
			task.wait(0.1)
		end
	end
end

local function CopyBodyColor(desc)
	if desc.bodyColors and desc.bodyColors.HeadColor then
		ReplicatedStorage.Remotes.ChangeBodyColor:FireServer(tostring(desc.bodyColors.HeadColor))
	end
end

local function GetPlayerByName(name)
	name = name:lower()
	for _, p in ipairs(Players:GetPlayers()) do
		if p ~= LocalPlayer and p.Name:lower():sub(1, #name) == name then
			return p
		end
	end
	return nil
end

local function GetDescription(player)
	if copyMode == "Brookhaven" then
		local humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
		if humanoid then
			return humanoid:GetAppliedDescription()
		end
	else
		return Players:GetHumanoidDescriptionFromUserId(player.UserId)
	end
end

local function CopyAvatarFrom(player)
	pcall(function()
		local desc = GetDescription(player)
		if not desc then return end

		CopyAccessories(LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):GetAppliedDescription())
		ResetBody()
		CopyAccessories(desc)
		task.wait(0.1)
		CopyBody(desc)
		CopyClothing(desc)
		CopyAnimations(desc)
		CopyBodyColor(desc)
	end)
end

-- إدخال اسم اللاعب
createTextBox(AvatarsTab, "اكتب اسم اللاعب...", function(txt)
	selectedPlayerName = txt
end)

-- اختيار نوع النسخ
createDropdown(AvatarsTab, {"في ماب البيوت", "اللسكن الحقيقي"}, "في ماب البيوت", function(opt)
	if opt == "في ماب البيوت" then
		copyMode = "Brookhaven"
	else
		copyMode = "Real"
	end
end)

-- زر: نسخ سكن اللاعب المحدد
createButton(AvatarsTab, "نسخ سكن اللاعب", function()
	if selectedPlayerName == "" then
		warn("الرجاء كتابة اسم اللاعب")
		return
	end
	local target = GetPlayerByName(selectedPlayerName)
	if target then
		CopyAvatarFrom(target)
	else
		warn("لم يتم العثور على اللاعب في السيرفر")
	end
end)

-- زر: نسخ سكن أقرب لاعب
createButton(AvatarsTab, "نسخ سكن لاعب قريب منك", function()
	local closest, minDist = nil, math.huge
	for _, p in pairs(Players:GetPlayers()) do
		if p ~= LocalPlayer and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
			local dist = (p.Character.HumanoidRootPart.Position - LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
			if dist < minDist then
				closest = p
				minDist = dist
			end
		end
	end
	if closest then
		CopyAvatarFrom(closest)
	end
end)

-- زر: نسخ سكن لاعب عشوائي
createButton(AvatarsTab, "نسخ سكن لاعب عشوائي", function()
	local list = {}
	for _, p in pairs(Players:GetPlayers()) do
		if p ~= LocalPlayer then
			table.insert(list, p)
		end
	end
	if #list > 0 then
		local randomPlayer = list[math.random(1, #list)]
		CopyAvatarFrom(randomPlayer)
	end
end)


-- قسم أنيميشنات الشخصية
createSection(AvatarsTab, "انيميشنات قديمة")

createButton(AvatarsTab, "أنيميشن R6 قديم", function()
	loadstring(game:HttpGet('https://gist.githubusercontent.com/Imperador950/f9e54330eb4a92331204aae37ec11aef/raw/db18d1c348beb8a79931346597137518966f2188/Byshelby'))()
end)

createButton(AvatarsTab, "أنيميشن R15 قديم", function()
	local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
	local humanoid = character:WaitForChild("Humanoid")

	for _, track in ipairs(humanoid:GetPlayingAnimationTracks()) do
		track:Stop()
		track:Destroy()
	end

	for _, desc in ipairs(character:GetDescendants()) do
		if desc:IsA("Animation") then
			desc:Destroy()
		end
	end

	for _, obj in ipairs(character:GetDescendants()) do
		if obj:IsA("Tool") or obj:IsA("LocalScript") or obj:IsA("ModuleScript") then
			for _, child in ipairs(obj:GetDescendants()) do
				if child:IsA("Animation") then
					child:Destroy()
				end
			end
		end
	end
end)
local PassesTab = createTab("القيم باسات")
createSection(PassesTab, "جميع أنواع القيم باسات")
local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")
local LocalPlayer = Players.LocalPlayer
local Bag = LocalPlayer:WaitForChild("PlayersBag")

local function SendNotify(title, icon)
    StarterGui:SetCore("SendNotification", {
        Title = title,
        Text = "",
        Icon = icon,
        Duration = 5
    })
end

createButton(PassesTab, "أحصل على التاج", function()
    SendNotify("أبشر يا قلبي تم الحصول على التاج", "rbxassetid://96510811373389")
    if Bag:FindFirstChild("VIP") then
        Bag.VIP.Value = true
    end
end)

createButton(PassesTab, "أحصل على الأوجه", function()
    SendNotify("أبشر يا قلبي تم الحصول على الاوجه", "rbxassetid://139444782526755")
    if Bag:FindFirstChild("FacePass") then
        Bag.FacePass.Value = true
    end
end)

createButton(PassesTab, "أحصل على النجمة", function()
    SendNotify("أبشر يا قلبي تم الحصول على النجمة", "rbxassetid://93100440000032")
    if Bag:FindFirstChild("SilverPass") then
        Bag.SilverPass.Value = true
    end
end)

createButton(PassesTab, "أحصل على النار", function()
    SendNotify("أبشر يا قلبي تم الحصول على النار", "rbxassetid://99197718536544")
    if Bag:FindFirstChild("FirePass") then
        Bag.FirePass.Value = true
    end
end)

createButton(PassesTab, "أحصل على السرعة", function()
    SendNotify("أبشر يا قلبي تم الحصول على السرعة", "rbxassetid://103085811778224")
    if Bag:FindFirstChild("SpeedPass200") then
        Bag.SpeedPass200.Value = true
    end
end)

createButton(PassesTab, "أحصل على الأغاني", function()
    SendNotify("أبشر يا قلبي تم الحصول على الأغاني", "rbxassetid://111924853884794")
    if Bag:FindFirstChild("MusicPass") then
        Bag.MusicPass.Value = true
    end
end)

local BypassesTab = createTab("الحمايات")
createSection(BypassesTab, "أستعمل الحمايات للوقاية من أي ضرر من اللاعبين")
createToggle(BypassesTab, "الحماية من البانق", false, function(Value)
   kiudi = Value
     while kiudi and task.wait() do
       for _, player in next, game.Players:GetPlayers() do
         if player ~= game.Players.LocalPlayer and game.Workspace:FindFirstChild(player.Name) and player.Character:FindFirstChild("Humanoid") and player.Character:FindFirstChild("HumanoidRootPart") and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - player.Character.HumanoidRootPart.Position).Magnitude <= 10 then
           for _, anim in next, player.Character.Humanoid:GetPlayingAnimationTracks() do
              if anim.Animation.AnimationId == "rbxassetid://148840371" or anim.Animation.AnimationId == "rbxassetid://5918726674" then
    game:GetService("RunService").Heartbeat:Wait()
    last_pick_sin_mrazy = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame 
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(9e8, workspace.FallenPartsDestroyHeight + 1, 9e8) 
    game:GetService("RunService").Heartbeat:Wait()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = last_pick_sin_mrazy
              end
           end
         end
       end
     end
 end)

createToggle(BypassesTab, "الحماية من الفلينق", false, function(Value)
 jhg = Value
     while jhg and task.wait() do
       for _, plr in next, game:GetService("Players"):GetPlayers() do
         if plr ~= game:GetService("Players").LocalPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
for _, part in next, plr.Character:GetChildren() do
  if part:IsA("BasePart") then
 part.CanCollide = not jhg
 part.CanTouch = not jhg
 part.AssemblyLinearVelocity = Vector3.zero
 part.AssemblyAngularVelocity = Vector3.zero
 part.Velocity = Vector3.zero
 part.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0)
  end
end
         end
       end
     end
 end)

createToggle(BypassesTab, "الحماية من الانتقال للفويد", false, function(Value)
       local function getrespawn(): Instance
  for _, part in next, workspace:GetDescendants() do
    if part:IsA("SpawnLocation") then
      return part
   end
  end
end

local thnei = Value
local char = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()

while thnei and task.wait() do
char = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
  if char:FindFirstChild("HumanoidRootPart") and char.HumanoidRootPart.CFrame.Y <= workspace.FallenPartsDestroyHeight + 10 then
    if getrespawn() then
 char.HumanoidRootPart.CFrame = getrespawn().CFrame
 char.HumanoidRootPart.Velocity = Vector3.zero
     else
 char.HumanoidRootPart.CFrame = CFrame.new(0, 0, 0)
 char.HumanoidRootPart.Velocity = Vector3.zero
    end
  end
end
 end)

createToggle(BypassesTab, "الحماية من الجلوس", false, function(Value)
          game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled("Seated", not Value)
 end)

local VirtualUser = game:GetService("VirtualUser")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local LocalPlayer = Players.LocalPlayer

local antiAfkConnection = nil
local heartbeatConnection = nil
local afkRemote = ReplicatedStorage:FindFirstChild("AFKRemote") -- غيّر الاسم إن وُجد
local afkFunction = ReplicatedStorage:FindFirstChild("AFKFunction") -- غيّر الاسم إن وُجد

createToggle(BypassesTab, "الحماية من الأفك", false, function(enabled)
         if enabled then
            -- VirtualUser لمنع الخروج بسبب عدم النشاط
            antiAfkConnection = LocalPlayer.Idled:Connect(function()
                VirtualUser:Button2Down(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
                task.wait(0.1)
                VirtualUser:Button2Up(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
                print(">> Sent VirtualUser to prevent AFK")
            end)

            -- Heartbeat loop لإرسال إشارات للـ RemoteEvent/Function
            heartbeatConnection = RunService.Heartbeat:Connect(function()
                -- Fire RemoteEvent إن وجد
                if afkRemote and afkRemote:IsA("RemoteEvent") then
                    pcall(function()
                        afkRemote:FireServer("anti_afk_ping")
                        print(">> Fired RemoteEvent")
                    end)
                end

                -- Invoke RemoteFunction إن وجد
                if afkFunction and afkFunction:IsA("RemoteFunction") then
                    pcall(function()
                        afkFunction:InvokeServer("anti_afk_check")
                        print(">> Invoked RemoteFunction")
                    end)
                end
            end)

            print("✅ ANTI-AFK Enabled.")
        else
            -- تنظيف الاتصالات عند التعطيل
            if antiAfkConnection then
                antiAfkConnection:Disconnect()
                antiAfkConnection = nil
            end

            if heartbeatConnection then
                heartbeatConnection:Disconnect()
                heartbeatConnection = nil
            end

            print("❌ ANTI-AFK Disabled.")
        end
 end)

createSection(BypassesTab, "المزيد قيد الأنشاء...")

