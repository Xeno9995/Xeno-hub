local LocalPlayer = Players.LocalPlayer
local Bag = LocalPlayer:WaitForChild("PlayersBag")

local function SendNotify(title, icon)
    StarterGui:SetCore("SendNotification", {
        Title = title,
        Text = "",
        Icon = icon,
        Duration = 5
    })
end

