-- Ø¯Ø§Ù„Ø© ØªØ­Ø¯ÙŠØ« Ø£Ø³Ù…Ø§Ø¡ Ø§Ù„Ù„Ø§Ø¹Ø¨ÙŠÙ†
local function GetPlayerNames()
    local names = {}
    for _, plr in ipairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer then
            table.insert(names, plr.Name)
        end
    end
    return names
end

-- Ø¯Ø§Ù„Ø© Ù„ØªØ­Ø¯ÙŠØ« Dropdown Ù…Ø¹ÙŠÙ†
local function RefreshPlayerDropdown(dropdownObject)
    dropdownObject:Refresh(GetPlayerNames(), true)
end

