-- تبويب اللاعب مع ESP وTeleport
local PlayerTab = createTab("اللاعب")
createSection(PlayerTab, "أختار لاعب للكشف عن مكانه و الانتقال أليه")

-- إعدادات ESP
local ESPSettings = {
    Enabled = false,
    BoxColor = Color3.fromRGB(0, 255, 0),
    TracerColor = Color3.fromRGB(255, 0, 0),
    TextColor = Color3.fromRGB(255, 255, 255),
    BoxThickness = 1.4,
    TracerThickness = 1.4,
    TeamCheck = false,
    Autothickness = true,
}

local ESPData = { Connections = {}, Objects = {} }
local selectedPlayer = nil

-- إنشاء ESP
local function CreateESP(player)
    if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") or not player.Character:FindFirstChild("Head") then return end
    local drawings = {
        Lines = {},
        Text = Drawing.new("Text"),
        Tracer = Drawing.new("Line")
    }

    for i = 1, 12 do
        local line = Drawing.new("Line")
        line.Visible = false
        line.Color = ESPSettings.BoxColor
        line.Thickness = ESPSettings.BoxThickness
        table.insert(drawings.Lines, line)
    end

    drawings.Text.Visible = false
    drawings.Text.Color = ESPSettings.TextColor
    drawings.Text.Size = 15
    drawings.Text.Center = true
    drawings.Text.Outline = true
    drawings.Text.OutlineColor = Color3.new(0,0,0)

    drawings.Tracer.Visible = false
    drawings.Tracer.Color = ESPSettings.TracerColor
    drawings.Tracer.Thickness = ESPSettings.TracerThickness

    local connection
    connection = RunService.RenderStepped:Connect(function()
        if not ESPSettings.Enabled or not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") or not player.Character:FindFirstChild("Head") then
            for _, line in pairs(drawings.Lines) do line.Visible = false end
            drawings.Tracer.Visible = false
            drawings.Text.Visible = false
            return
        end

        local rootPart = player.Character.HumanoidRootPart
        local head = player.Character.Head
        local pos, vis = Camera:WorldToViewportPoint(rootPart.Position)
        if vis then
            local scale = head.Size.Y / 2
            local size = Vector3.new(2, 3, 1.5) * (scale * 2)
            local points = {
                Top = {
                    Camera:WorldToViewportPoint((rootPart.CFrame * CFrame.new(-size.X, size.Y, -size.Z)).p),
                    Camera:WorldToViewportPoint((rootPart.CFrame * CFrame.new(-size.X, size.Y, size.Z)).p),
                    Camera:WorldToViewportPoint((rootPart.CFrame * CFrame.new(size.X, size.Y, size.Z)).p),
                    Camera:WorldToViewportPoint((rootPart.CFrame * CFrame.new(size.X, size.Y, -size.Z)).p),
                },
                Bottom = {
                    Camera:WorldToViewportPoint((rootPart.CFrame * CFrame.new(-size.X, -size.Y, -size.Z)).p),
                    Camera:WorldToViewportPoint((rootPart.CFrame * CFrame.new(-size.X, -size.Y, size.Z)).p),
                    Camera:WorldToViewportPoint((rootPart.CFrame * CFrame.new(size.X, -size.Y, size.Z)).p),
                    Camera:WorldToViewportPoint((rootPart.CFrame * CFrame.new(size.X, -size.Y, -size.Z)).p),
                }
            }

            for i = 1, 4 do
                drawings.Lines[i].From = Vector2.new(points.Top[i].X, points.Top[i].Y)
                drawings.Lines[i].To = Vector2.new(points.Top[(i % 4) + 1].X, points.Top[(i % 4) + 1].Y)
                drawings.Lines[i+4].From = Vector2.new(points.Bottom[i].X, points.Bottom[i].Y)
                drawings.Lines[i+4].To = Vector2.new(points.Bottom[(i % 4) + 1].X, points.Bottom[(i % 4) + 1].Y)
                drawings.Lines[i+8].From = Vector2.new(points.Bottom[i].X, points.Bottom[i].Y)
                drawings.Lines[i+8].To = Vector2.new(points.Top[i].X, points.Top[i].Y)
            end

            if ESPSettings.Autothickness then
                local dist = (LocalPlayer.Character.HumanoidRootPart.Position - rootPart.Position).Magnitude
                local thickness = math.clamp(1 / dist * 100, 0.1, 4)
                for _, line in pairs(drawings.Lines) do line.Thickness = thickness end
                drawings.Tracer.Thickness = thickness
            else
                for _, line in pairs(drawings.Lines) do line.Thickness = ESPSettings.BoxThickness end
                drawings.Tracer.Thickness = ESPSettings.TracerThickness
            end

            local tracerPos = Camera:WorldToViewportPoint((rootPart.CFrame * CFrame.new(0, -size.Y, 0)).p)
            drawings.Tracer.From = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y)
            drawings.Tracer.To = Vector2.new(tracerPos.X, tracerPos.Y)

            local distance = math.floor((LocalPlayer.Character.HumanoidRootPart.Position - rootPart.Position).Magnitude)
            drawings.Text.Text = player.Name .. "\n" .. distance .. "m"
            drawings.Text.Position = Vector2.new(pos.X, pos.Y - 50)

            for _, line in pairs(drawings.Lines) do line.Visible = true end
            drawings.Tracer.Visible = true
            drawings.Text.Visible = true
        else
            for _, line in pairs(drawings.Lines) do line.Visible = false end
            drawings.Tracer.Visible = false
            drawings.Text.Visible = false
        end
    end)

    table.insert(ESPData.Connections, connection)
    ESPData.Objects[player] = drawings
end

-- حذف جميع ESP
local function ClearESP()
    for _, conn in pairs(ESPData.Connections) do pcall(function() conn:Disconnect() end) end
    ESPData.Connections = {}
    for _, drawings in pairs(ESPData.Objects) do
        for _, line in pairs(drawings.Lines) do pcall(function() line:Remove() end) end
        pcall(function() drawings.Text:Remove() end)
        pcall(function() drawings.Tracer:Remove() end)
    end
    ESPData.Objects = {}
end

-- dropdown لاختيار لاعب
local espDropdown = createDropdown(PlayerTab, GetPlayerNames(), nil, function(choice)
    selectedPlayer = Players:FindFirstChild(choice)
    ClearESP()
    if ESPSettings.Enabled and selectedPlayer then
        CreateESP(selectedPlayer)
    end
end)

Players.PlayerAdded:Connect(function()
    task.wait(1)
    RefreshPlayerDropdown(espDropdown)
    RefreshPlayerDropdown(orbitDropdown)
end)
Players.PlayerRemoving:Connect(function()
    task.wait(1)
    RefreshPlayerDropdown(espDropdown)
    RefreshPlayerDropdown(orbitDropdown)
end)

-- Toggle ESP
createToggle(PlayerTab, "كشف مكان اللاعب", false, function(state)
    ESPSettings.Enabled = state
    if state and selectedPlayer then CreateESP(selectedPlayer) else ClearESP() end
end)

-- دوران الكاميرا حول اللاعب
local orbitEnabled = false
local orbitConnection = nil
local orbitRadius = 10
local orbitSpeed = 2

local function StartOrbit()
    if not selectedPlayer or not selectedPlayer.Character or not selectedPlayer.Character:FindFirstChild("Head") then return end
    local angle = 0
    orbitConnection = RunService.RenderStepped:Connect(function(dt)
        if not orbitEnabled then return end
        if not selectedPlayer or not selectedPlayer.Character or not selectedPlayer.Character:FindFirstChild("Head") then return end
        angle = angle + orbitSpeed * dt
        local targetPos = selectedPlayer.Character.Head.Position
        local offset = Vector3.new(math.cos(angle), 0.3, math.sin(angle)) * orbitRadius
        local cameraPos = targetPos + offset
        Camera.CameraType = Enum.CameraType.Scriptable
        Camera.CFrame = CFrame.new(cameraPos, targetPos)
    end)
end

local function StopOrbit()
    if orbitConnection then orbitConnection:Disconnect() orbitConnection = nil end
    Camera.CameraType = Enum.CameraType.Custom
end

createToggle(PlayerTab, "رؤية الشخصية", false, function(state)
    orbitEnabled = state
    if state then StartOrbit() else StopOrbit() end
end)

-- Teleport
createButton(PlayerTab, "الأنتقال", function()
    if selectedPlayer and selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("HumanoidRootPart") then
        LocalPlayer.Character:MoveTo(selectedPlayer.Character.HumanoidRootPart.Position)
    end
end)

-- تحديث Dropdown كل ثانية تلقائيًا لضمان تحديث قائمة اللاعبين بشكل دائم
task.spawn(function()
    while true do
        task.wait(1)
        RefreshPlayerDropdown(espDropdown)
        RefreshPlayerDropdown(orbitDropdown)
    end
end)

createSection(PlayerTab, "المشي عبر الجدران")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local noclipConnection
local noclipEnabled = false

createToggle(PlayerTab, "اختراق الجدران", false, function(state)
noclipEnabled = state

        if noclipEnabled then
            noclipConnection = RunService.Stepped:Connect(function()
                if player.Character and player.Character:FindFirstChild("Humanoid") then
                    for _, part in ipairs(player.Character:GetDescendants()) do
                        if part:IsA("BasePart") and part.CanCollide == true then
                            part.CanCollide = false
                        end
                    end
                end
            end)
            print("🚀 Noclip Enabled")
        else
            if noclipConnection then
                noclipConnection:Disconnect()
                noclipConnection = nil
            end

            -- إعادة التفعيل للتصادم عند إيقاف Noclip
            if player.Character then
                for _, part in ipairs(player.Character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = true
                    end
                end
            end
            print("🛑 Noclip Disabled")
        end
 end)
createButton(PlayerTab, "تفعيل يد نارية", function()
    local character = player.Character
    if not character then return end

    local humanoid = character:FindFirstChildOfClass("Humanoid")
    local hrp = character:FindFirstChild("HumanoidRootPart")
    if not humanoid or not hrp then return end

    local oldPos = hrp.CFrame
    local firstPos = CFrame.new(-349, 5, 98)

    local poolClick = Workspace:FindFirstChild("WorkspaceCom"):FindFirstChild("001_Hospital"):FindFirstChild("PoolClick")
    if poolClick and poolClick:FindFirstChild("ClickDetector") then
        humanoid.WalkSpeed = 0
        humanoid.JumpPower = 0

        hrp.CFrame = firstPos
        task.wait(1)
        hrp.CFrame = poolClick.CFrame
        fireclickdetector(poolClick.ClickDetector)
        task.wait(0.7)
        hrp.CFrame = oldPos

        humanoid.WalkSpeed = 16
        humanoid.JumpPower = 50

        notify("Trail", "Trail obtido com sucesso!")
    else
        warn("PoolClick ou ClickDetector não encontrado!")
        notify("Erro", "Trail não encontrado.")
    end
end)
createSection(PlayerTab, "تفعيل جميع الأدوات في يدك")
createButton(PlayerTab, "تفعيل الأدوات", function()
LP = game.Players.LocalPlayer
for _, tool in ipairs(LP.Backpack:GetDescendants()) do
  if tool:IsA("Tool") then
tool.Parent = LP.Character
  end
end
 print("Equiped!")
 end)

createSection(PlayerTab, "حذف جميع الادوات في يدك")
createButton(PlayerTab, "حذف الأدوات", function()
	if table.merger == nil then
      loadstring(game:HttpGet("https://raw.githubusercontent.com/ADSKerOffical/BHU/refs/heads/main/Table%20library%20expanding"))()
    end
for _, tool in next, table.merger(game.Players.LocalPlayer.Character:GetDescendants(), game.Players.LocalPlayer.Backpack:GetDescendants()) do
  if tool:IsA("Tool") then
    tool:Destroy()
  end
end
 print("Removed!")
 end)
 
createSection(PlayerTab, "رؤية أي شيء خلف الجدران")
createToggle(PlayerTab, "تفعيل رؤية عبر الجدران", false, function(Value)
 xrayen = Value
for _, part in ipairs(workspace:GetDescendants()) do
  if part:IsA("BasePart") and not part.Parent:FindFirstChildOfClass("Humanoid") and part.Parent ~= game.Players.LocalPlayer.Character then
 part.LocalTransparencyModifier = xrayen and 0.6 or 0
  end
end
 end)

createSection(PlayerTab, "أذا جلس اللاعب على كنبة مثلا و تركتها يطير")
local hiddenfling = false
createToggle(PlayerTab, "طيران اللاعب باللمس", false, function(value)
 hiddenfling = value
            local hrp, c, vel, movel = nil, nil, nil, 0.1
            local player = game.Players.LocalPlayer

            while player and player.Character and player.Character:FindFirstChild("Humanoid") and player.Character.Humanoid.Health > 0 and hiddenfling == true do
                game:GetService("RunService").Heartbeat:Wait()

                local lp = player
                while hiddenfling == true and not (c and c.Parent and hrp and hrp.Parent) do
                    game:GetService("RunService").Heartbeat:Wait()
                    c = lp.Character
                    hrp = c:FindFirstChild("HumanoidRootPart") or c:FindFirstChild("Torso") or c:FindFirstChild("UpperTorso")
                end

                if hiddenfling == true then
                    vel = hrp.Velocity
                    hrp.Velocity = vel * 10000 + Vector3.new(0, 100000, 0)
                    game:GetService("RunService").RenderStepped:Wait()

                    if c and c.Parent and hrp and hrp.Parent then
                        hrp.Velocity = vel
                    end

                    game:GetService("RunService").Stepped:Wait()

                    if c and c.Parent and hrp and hrp.Parent then
                        hrp.Velocity = vel + Vector3.new(0, movel, 0)
                        movel = movel * -1
                    end
                end
            end
 end)
 
createSection(PlayerTab, "سكربت طيران المطور نيبرو")
createButton(PlayerTab, "تفعيل سكربت الطيران", function()
       local main = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local up = Instance.new("TextButton")
local down = Instance.new("TextButton")
local onof = Instance.new("TextButton")
