createSection(AvatarsTab, "الرؤوس المطلوبة")
local skins = {
    ["رآس مخفي"] = 134082579,
    ["عيون زرقاء"] = 16580493236,
    ["رأس الفضائي"] = 3210773801,
}
local selectedSkinName = nil
createDropdown(AvatarsTab, {"رأس مخفي", "عيون زرقاء", "رأس الفضائي"}, nil, function(value)
    selectedSkinName = value
end)
createButton(AvatarsTab, "تفعيل اللسكن", function()
    if selectedSkinName and skins[selectedSkinName] then
        local args = {
            [1] = {0, 0, 0, 0, 0, skins[selectedSkinName]}
        }
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("ChangeCharacterBody"):InvokeServer(unpack(args))
    else
        warn("Please select a skin first.")
    end
end)

