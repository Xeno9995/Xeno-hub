local RepStorage = game:GetService("ReplicatedStorage")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local LocalPlayer = Players.LocalPlayer
local Camera = workspace.CurrentCamera

--==[ State Variables & Connections ]==--
local selectedPlayerName = nil
local toggleConns = {} -- Table to keep track of toggles' connections

--==[ Utility Functions ]==--
local function GetPlayerNames()
    local t = {}
    for _, p in ipairs(Players:GetPlayers()) do
        if p ~= LocalPlayer then table.insert(t, p.Name) end
    end
    return t
end

local function getTargetPlayer()
    return selectedPlayerName and Players:FindFirstChild(selectedPlayerName) or nil
end

local function disconnectToggle(key)
    if toggleConns[key] then
        toggleConns[key]:Disconnect()
        toggleConns[key] = nil
    end
end

--==[ Dropdown: Player Selection ]==--
local dropdown = createDropdown(TrollTab, GetPlayerNames(), nil, function(option)
    selectedPlayerName = option
end)
Players.PlayerAdded:Connect(function() dropdown:Refresh(GetPlayerNames(), true) end)
Players.PlayerRemoving:Connect(function() dropdown:Refresh(GetPlayerNames(), true) end)

--==[ Spectate Section ]==--
createSection(TrollTab, "أختار لاعب لقتله أو مشاهدته")
createToggle(TrollTab, "مشاهدة", false, function(on)
    disconnectToggle("spectate")
    if on then
        if not selectedPlayerName then return warn("اختر لاعب أولاً") end
        toggleConns["spectate"] = RunService.RenderStepped:Connect(function()
            local target = getTargetPlayer()
            if target and target.Character and target.Character:FindFirstChild("Humanoid") then
                Camera.CameraSubject = target.Character.Humanoid
            end
        end)
    else
        Camera.CameraSubject = LocalPlayer.Character:FindFirstChild("Humanoid")
    end
end)

--==[ Kill Player Section ]==--
createSection(TrollTab, "قتل اللاعب اللي أخترته")
createToggle(TrollTab, "قتل", false, function(on)
    disconnectToggle("Kill")
    if on then
        toggleConns["Kill"] = RunService.Heartbeat:Connect(function()
            if not selectedPlayerName then return end
            local target = getTargetPlayer()
            local char = LocalPlayer.Character
            if target and target.Character and char then
                local tool = char:FindFirstChildOfClass("Tool")
                local seat = tool and tool:FindFirstChildWhichIsA("Seat", true)
                if seat then
                    char.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame * CFrame.new(0,-2,0)
                    seat:Sit(target.Character:FindFirstChildOfClass("Humanoid"))
                    if target.Character:FindFirstChildOfClass("Humanoid").SeatPart == seat then
                        target.Character.HumanoidRootPart.CFrame = CFrame.new(0,-5000,0)
                        target.Character.Humanoid.Health = 0
                        disconnectToggle("kill")
                        notify("Player Killed ✅")
                    end
                end
            end
        end)
    end
end)

--==[ Bang Moves Section ]==--
createSection(TrollTab, "بانق اللاعب")
local bangStyles = {
    {name = "بانق وجه 1", speed=0.3, easing=Enum.EasingStyle.Sine},
    {name = "بانق وجه 2", speed=0.1, easing=Enum.EasingStyle.Linear},
    {name = "بانق خلفي 1", offset=1.2, speed=0.2},
    {name = "بانق خلفي 2", offset=1.2, speed=0.1}
}
for i, style in ipairs(bangStyles) do
    createToggle(TrollTab, style.name, false, function(on)
        disconnectToggle("bang"..i)
        if on and selectedPlayerName then
            local moving = true
            toggleConns["bang"..i] = RunService.Heartbeat:Connect(function()
                local char = LocalPlayer.Character
                local root = char and char:FindFirstChild("HumanoidRootPart")
                local target = getTargetPlayer()
                local targetRoot = target and target.Character and target.Character:FindFirstChild("HumanoidRootPart")
                if root and targetRoot then
                    local baseCFrame = style.offset
                        and CFrame.new(targetRoot.Position - targetRoot.CFrame.LookVector*style.offset, targetRoot.Position)
                        or CFrame.new(targetRoot.Position + targetRoot.CFrame.LookVector*2, targetRoot.Position)
                    root.CFrame = baseCFrame
                    local offsetVal = moving and ((style.offset and 0.5) or 1) or ((style.offset and -0.5) or -0.7)
                    local tw = TweenService:Create(root, TweenInfo.new(style.speed, style.easing or Enum.EasingStyle.Sine), {CFrame = root.CFrame * CFrame.new(0,0,offsetVal)})
                    tw:Play()
                    tw.Completed:Once(function() moving = not moving end)
                end
            end)
        else
            local hum = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
            if hum then hum.Sit = false end
        end
    end)
end

--==[ Fling & Sit Section ]==--
createSection(TrollTab, "فلينق & جلوس اللاعب")

-- Fling
createToggle(TrollTab, "فلينق", false, function(on)
    disconnectToggle("Fling")
    if on and selectedPlayerName then
        toggleConns["Fling"] = RunService.Heartbeat:Connect(function()
            local char = LocalPlayer.Character
            local tool = char and char:FindFirstChildOfClass("Tool")
            local target = getTargetPlayer()
            local tr = target and target.Character and target.Character:FindFirstChild("HumanoidRootPart")
            local root = char and char:FindFirstChild("HumanoidRootPart")
            if tool and root and tr then
                root.CFrame = tr.CFrame * CFrame.new(0,-3,0)
                local seat = tool:FindFirstChildWhichIsA("Seat", true)
                if seat then seat:Sit(target.Character:FindFirstChildOfClass("Humanoid")); task.wait(0.2); tr.Velocity = Vector3.new(0,100,0) end
            end
        end)
    end
end)

-- Sit on Head
createToggle(TrollTab, "الجلوس فوق رأس اللاعب", false, function(on)
    disconnectToggle("sithead")
    if on and selectedPlayerName then
        local hum = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
        if hum then hum.Sit = true end
        toggleConns["sithead"] = RunService.Heartbeat:Connect(function()
            local target = getTargetPlayer()
            local head = target and target.Character and target.Character:FindFirstChild("Head")
            local root = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
            if root and head then root.CFrame = head.CFrame * CFrame.new(0,1.25,0) end
        end)
    else
        local hum = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
        if hum then hum.Sit = false end
    end
end)

--==[ Utilities Section ]==--
createSection(TrollTab, "أنسخ أسم اللاعب اللي أخترته")
createButton(TrollTab, "نسخ أسم اللاعب", function()
    if selectedPlayerName then setclipboard(selectedPlayerName); notify("Copied: "..selectedPlayerName) end
end)
createSection(TrollTab, "دوران الشخصية")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local LocalPlayer = Players.LocalPlayer
local SpinToggle = false
local SpinConnection = nil

-- ✅ RemoteEvent للتجربة (استبدله بريموتك الحقيقي إذا عندك)
local Remote = ReplicatedStorage:FindFirstChild("FakeRemote")
if not Remote then
    Remote = Instance.new("RemoteEvent", ReplicatedStorage)
    Remote.Name = "FakeRemote"
end

-- ✅ دالة الدوران
local function StartSpinning()
    local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    local Root = Character:WaitForChild("HumanoidRootPart")

    local angle = 0
    SpinConnection = RunService.Heartbeat:Connect(function(dt)
        if not SpinToggle then return end
        angle = angle + (dt * math.rad(360))
        Root.CFrame = Root.CFrame * CFrame.Angles(0, math.rad(5), 0)

        -- مثال على FireServer باستخدام unpack(args)
        local args = {
            "SpinningPlayer", LocalPlayer.Name, tick()
        }
        Remote:FireServer(unpack(args))
    end)
end

-- ✅ دالة الإيقاف
local function StopSpinning()
    if SpinConnection then
        SpinConnection:Disconnect()
        SpinConnection = nil
    end
end

-- ✅ زر Toggle داخل التاب (مثلاً Tab1 أو Tab5)
createToggle(TrollTab, "دوران", false, function(state)
 SpinToggle = state
        if state then
            StartSpinning()
        else
            StopSpinning()
        end
 end)
 
createSection(TrollTab, "فعل أسماء تقهر للاعب المستهدف")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Remote = ReplicatedStorage:WaitForChild("RE"):WaitForChild("1RPNam1eTex1t")

local RoleplayNames = {
    "انا عمك",
    "اللي قدامي سقايط",
    "يا مثرثر",
    "مدعوس في كرامتك",
    "يا واطي",
    "يا 66 جحبة",
   "أقهرك",
    "أمك",
    "زاكية",
    "خاينة",
    "زاينة",
    "بن المزيونة",
   "بن المخشبة",
    "يا زاين",
    "يا ساقط",
    "تجيب فزعات خايف",
    "يا بنت"
}

local Toggle = false
local ChangeLoop

-- ✅ دالة تغيير الاسم
local function StartChangingNames()
    local i = 1
    ChangeLoop = task.spawn(function()
        while Toggle do
            local args = {
                [1] = "RolePlayName",
                [2] = RoleplayNames[i]
            }
            Remote:FireServer(unpack(args))
            i = i + 1
            if i > #RoleplayNames then
                i = 1
            end
            task.wait(0.4) -- الوقت بين كل تغيير، عدّله لو حبيت أسرع
        end
    end)
end

local function StopChangingNames()
    Toggle = false
end

-- ✅ زر Toggle (اربطه في UI Library أو شغّله يدوي)
createToggle(TrollTab, "تفعيل الأسماء العشوائية", false, function(state)
         Toggle = state
        if state then
            StartChangingNames()
        else
            StopChangingNames()
        end
 end)

