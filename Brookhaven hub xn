-- Normal Name Section
createSection(AvatarsTab, "الأسم مع التلوين")
createTextBox(AvatarsTab, "أكتب الأسم...", function(value)
    local args = {"RolePlayName", value}
    local remote = game:GetService("ReplicatedStorage").RE:FindFirstChild("1RPNam1eTex1t")
    if remote then
        remote:FireServer(unpack(args))
    end
end)

-- Rainbow Name Toggle
local rainbowNameRunning = false

createToggle(AvatarsTab, "تلوين الأسم", false, function(state)
    rainbowNameRunning = state
    if state then
        task.spawn(function()
            while rainbowNameRunning do
                local hue = (tick() % 10) / 10
                local color = Color3.fromHSV(hue, 1, 1)
                local args = {"PickingRPNameColor", color}
                local remote = game:GetService("ReplicatedStorage"):WaitForChild("RE"):FindFirstChild("1RPNam1eColo1r")
                if remote then
                    remote:FireServer(unpack(args))
                end
                task.wait(0.1)
            end
        end)
    end
end)

