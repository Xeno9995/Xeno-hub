-- Ù‚Ø³Ù…: Ù†Ø³Ø® Ø³ÙƒÙ†Ø§Øª Ø§Ù„Ù„Ø§Ø¹Ø¨ÙŠÙ† FE
createSection(AvatarsTab, "Ù†Ø³Ø® Ø³ÙƒÙ†Ø§Øª Ø§Ù„Ù„Ø§Ø¹Ø¨ÙŠÙ† FE")

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local LocalPlayer = Players.LocalPlayer

local selectedPlayerName = ""
local copyMode = "Brookhaven" -- "Brookhaven" = ÙÙŠ Ù…Ø§Ø¨ Ø§Ù„Ø¨ÙŠÙˆØª | "Real" = Ø§Ù„Ù„Ø³ÙƒÙ† Ø§Ù„Ø­Ù‚ÙŠÙ‚ÙŠ

-- ÙˆØ¸Ø§Ø¦Ù Ø§Ù„Ù…Ø³Ø§Ø¹Ø¯Ø©
local function WearAsset(id)
	pcall(function()
		ReplicatedStorage.Remotes.Wear:InvokeServer(tonumber(id))
	end)
end

local function ResetBody()
	pcall(function()
		ReplicatedStorage.Remotes.ChangeCharacterBody:InvokeServer({0, 0, 0, 0, 0, 0}, "AllBlocky")
	end)
end

local function CopyAccessories(desc)
	for _, accessory in ipairs(desc:GetAccessories(true)) do
		if accessory.AssetId then
			WearAsset(accessory.AssetId)
			task.wait(0.1)
		end
	end
end

local function CopyBody(desc)
	local parts = {
		tonumber(desc.Torso),
		tonumber(desc.RightArm),
		tonumber(desc.LeftArm),
		tonumber(desc.RightLeg),
		tonumber(desc.LeftLeg),
		tonumber(desc.Head),
	}
	ReplicatedStorage.Remotes.ChangeCharacterBody:InvokeServer(parts, "ShnmaxHub")
end

local function CopyClothing(desc)
	for _, asset in ipairs({desc.Shirt, desc.Pants, desc.GraphicTShirt, desc.Face}) do
		if tonumber(asset) and asset ~= 0 then
			WearAsset(asset)
			task.wait(0.1)
		end
	end
end

local function CopyAnimations(desc)
	for _, anim in ipairs({
		desc.IdleAnimation,
		desc.WalkAnimation,
		desc.RunAnimation,
		desc.JumpAnimation,
		desc.FallAnimation,
		desc.SwimAnimation
	}) do
		if tonumber(anim) and anim ~= 0 then
			WearAsset(anim)
			task.wait(0.1)
		end
	end
end

local function CopyBodyColor(desc)
	if desc.bodyColors and desc.bodyColors.HeadColor then
		ReplicatedStorage.Remotes.ChangeBodyColor:FireServer(tostring(desc.bodyColors.HeadColor))
	end
end

local function GetPlayerByName(name)
	name = name:lower()
	for _, p in ipairs(Players:GetPlayers()) do
		if p ~= LocalPlayer and p.Name:lower():sub(1, #name) == name then
			return p
		end
	end
	return nil
end

local function GetDescription(player)
	if copyMode == "Brookhaven" then
		local humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
		if humanoid then
			return humanoid:GetAppliedDescription()
		end
	else
		return Players:GetHumanoidDescriptionFromUserId(player.UserId)
	end
end

local function CopyAvatarFrom(player)
	pcall(function()
		local desc = GetDescription(player)
		if not desc then return end

		CopyAccessories(LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):GetAppliedDescription())
		ResetBody()
		CopyAccessories(desc)
		task.wait(0.1)
		CopyBody(desc)
		CopyClothing(desc)
		CopyAnimations(desc)
		CopyBodyColor(desc)
	end)
end

-- Ø¥Ø¯Ø®Ø§Ù„ Ø§Ø³Ù… Ø§Ù„Ù„Ø§Ø¹Ø¨
createTextBox(AvatarsTab, "Ø§ÙƒØªØ¨ Ø§Ø³Ù… Ø§Ù„Ù„Ø§Ø¹Ø¨...", function(txt)
	selectedPlayerName = txt
end)

-- Ø§Ø®ØªÙŠØ§Ø± Ù†ÙˆØ¹ Ø§Ù„Ù†Ø³Ø®
createDropdown(AvatarsTab, {"ÙÙŠ Ù…Ø§Ø¨ Ø§Ù„Ø¨ÙŠÙˆØª", "Ø§Ù„Ù„Ø³ÙƒÙ† Ø§Ù„Ø­Ù‚ÙŠÙ‚ÙŠ"}, "ÙÙŠ Ù…Ø§Ø¨ Ø§Ù„Ø¨ÙŠÙˆØª", function(opt)
	if opt == "ÙÙŠ Ù…Ø§Ø¨ Ø§Ù„Ø¨ÙŠÙˆØª" then
		copyMode = "Brookhaven"
	else
		copyMode = "Real"
	end
end)

-- Ø²Ø±: Ù†Ø³Ø® Ø³ÙƒÙ† Ø§Ù„Ù„Ø§Ø¹Ø¨ Ø§Ù„Ù…Ø­Ø¯Ø¯
createButton(AvatarsTab, "Ù†Ø³Ø® Ø³ÙƒÙ† Ø§Ù„Ù„Ø§Ø¹Ø¨", function()
	if selectedPlayerName == "" then
		warn("Ø§Ù„Ø±Ø¬Ø§Ø¡ ÙƒØªØ§Ø¨Ø© Ø§Ø³Ù… Ø§Ù„Ù„Ø§Ø¹Ø¨")
		return
	end
	local target = GetPlayerByName(selectedPlayerName)
	if target then
		CopyAvatarFrom(target)
	else
		warn("Ù„Ù… ÙŠØªÙ… Ø§Ù„Ø¹Ø«ÙˆØ± Ø¹Ù„Ù‰ Ø§Ù„Ù„Ø§Ø¹Ø¨ ÙÙŠ Ø§Ù„Ø³ÙŠØ±ÙØ±")
	end
end)

-- Ø²Ø±: Ù†Ø³Ø® Ø³ÙƒÙ† Ø£Ù‚Ø±Ø¨ Ù„Ø§Ø¹Ø¨
createButton(AvatarsTab, "Ù†Ø³Ø® Ø³ÙƒÙ† Ù„Ø§Ø¹Ø¨ Ù‚Ø±ÙŠØ¨ Ù…Ù†Ùƒ", function()
	local closest, minDist = nil, math.huge
	for _, p in pairs(Players:GetPlayers()) do
		if p ~= LocalPlayer and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
			local dist = (p.Character.HumanoidRootPart.Position - LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
			if dist < minDist then
				closest = p
				minDist = dist
			end
		end
	end
	if closest then
		CopyAvatarFrom(closest)
	end
end)

-- Ø²Ø±: Ù†Ø³Ø® Ø³ÙƒÙ† Ù„Ø§Ø¹Ø¨ Ø¹Ø´ÙˆØ§Ø¦ÙŠ
createButton(AvatarsTab, "Ù†Ø³Ø® Ø³ÙƒÙ† Ù„Ø§Ø¹Ø¨ Ø¹Ø´ÙˆØ§Ø¦ÙŠ", function()
	local list = {}
	for _, p in pairs(Players:GetPlayers()) do
		if p ~= LocalPlayer then
			table.insert(list, p)
		end
	end
	if #list > 0 then
		local randomPlayer = list[math.random(1, #list)]
		CopyAvatarFrom(randomPlayer)
	end
end)


-- Ù‚Ø³Ù… Ø£Ù†ÙŠÙ…ÙŠØ´Ù†Ø§Øª Ø§Ù„Ø´Ø®ØµÙŠØ©
