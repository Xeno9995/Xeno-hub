for _, part in next, plr.Character:GetChildren() do
  if part:IsA("BasePart") then
 part.CanCollide = not jhg
 part.CanTouch = not jhg
 part.AssemblyLinearVelocity = Vector3.zero
 part.AssemblyAngularVelocity = Vector3.zero
 part.Velocity = Vector3.zero
 part.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0)
  end
end
         end
       end
     end
   end    
})
Tab11:AddToggle({
 Name = "Anti Void",
 Default = false,
 Callback = function(Value)
      local function getrespawn(): Instance
  for _, part in next, workspace:GetDescendants() do
    if part:IsA("SpawnLocation") then
      return part
   end
  end
end

local thnei = Value
local char = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()

while thnei and task.wait() do
char = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
  if char:FindFirstChild("HumanoidRootPart") and char.HumanoidRootPart.CFrame.Y <= workspace.FallenPartsDestroyHeight + 10 then
    if getrespawn() then
 char.HumanoidRootPart.CFrame = getrespawn().CFrame
 char.HumanoidRootPart.Velocity = Vector3.zero
     else
 char.HumanoidRootPart.CFrame = CFrame.new(0, 0, 0)
 char.HumanoidRootPart.Velocity = Vector3.zero
    end
  end
end
   end    
})
Tab11:AddToggle({
 Name = "Anti sit",
 Default = false,
 Callback = function(Value)
         game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled("Seated", not Value)
   end    
})
local VirtualUser = game:GetService("VirtualUser")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local LocalPlayer = Players.LocalPlayer

local antiAfkConnection = nil
local heartbeatConnection = nil
local afkRemote = ReplicatedStorage:FindFirstChild("AFKRemote") -- ØºÙŠÙ‘Ø± Ø§Ù„Ø§Ø³Ù… Ø¥Ù† ÙˆÙØ¬Ø¯
local afkFunction = ReplicatedStorage:FindFirstChild("AFKFunction") -- ØºÙŠÙ‘Ø± Ø§Ù„Ø§Ø³Ù… Ø¥Ù† ÙˆÙØ¬Ø¯

Tab11:AddToggle({
    Name = "Anti Afk",
    Default = false,
    Callback = function(enabled)
        if enabled then
            -- VirtualUser Ù„Ù…Ù†Ø¹ Ø§Ù„Ø®Ø±ÙˆØ¬ Ø¨Ø³Ø¨Ø¨ Ø¹Ø¯Ù… Ø§Ù„Ù†Ø´Ø§Ø·
            antiAfkConnection = LocalPlayer.Idled:Connect(function()
                VirtualUser:Button2Down(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
                task.wait(0.1)
                VirtualUser:Button2Up(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
                print(">> Sent VirtualUser to prevent AFK")
            end)

            -- Heartbeat loop Ù„Ø¥Ø±Ø³Ø§Ù„ Ø¥Ø´Ø§Ø±Ø§Øª Ù„Ù„Ù€ RemoteEvent/Function
            heartbeatConnection = RunService.Heartbeat:Connect(function()
                -- Fire RemoteEvent Ø¥Ù† ÙˆØ¬Ø¯
                if afkRemote and afkRemote:IsA("RemoteEvent") then
                    pcall(function()
                        afkRemote:FireServer("anti_afk_ping")
                        print(">> Fired RemoteEvent")
                    end)
                end

                -- Invoke RemoteFunction Ø¥Ù† ÙˆØ¬Ø¯
                if afkFunction and afkFunction:IsA("RemoteFunction") then
                    pcall(function()
                        afkFunction:InvokeServer("anti_afk_check")
                        print(">> Invoked RemoteFunction")
                    end)
                end
            end)

            print("âœ… ANTI-AFK Enabled.")
        else
            -- ØªÙ†Ø¸ÙŠÙ Ø§Ù„Ø§ØªØµØ§Ù„Ø§Øª Ø¹Ù†Ø¯ Ø§Ù„ØªØ¹Ø·ÙŠÙ„
            if antiAfkConnection then
                antiAfkConnection:Disconnect()
                antiAfkConnection = nil
            end

            if heartbeatConnection then
                heartbeatConnection:Disconnect()
                heartbeatConnection = nil
            end

            print("âŒ ANTI-AFK Disabled.")
        end
    end
})
local Section = Tab11:AddSection({"More Coming Soon..."})
