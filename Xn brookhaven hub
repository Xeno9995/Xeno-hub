                                if hum then hum.Health = 0 end
                                XenoLib:Notify("âœ… Ø§Ù„Ø­Ø§ÙÙ„Ø© ÙˆØ§Ù„Ù„Ø§Ø¹Ø¨ ØªÙ… Ù†Ù‚Ù„Ù‡Ù…Ø§ Ù„Ù„Ø­Ù‚Ù„ ÙˆÙ‚ØªÙ„Ù‡Ù…Ø§.")
                                autoKillRunning = false
                            end
                            return
                        end
                    end
                end
            end)
        else
            if loopConn then
                loopConn:Disconnect()
                loopConn = nil
            end
            XenoLib:Notify("ğŸ›‘ ØªÙ… Ø¥ÙŠÙ‚Ø§Ù Kill Player.")
        end
    end
})
