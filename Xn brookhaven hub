                spawnRemote:FireServer({ VehicleName = "School Bus" })
                XenoLib:Notify("ğŸš ØªÙ… Ø§Ø³ØªØ¯Ø¹Ø§Ø¡ Ø§Ù„Ø­Ø§ÙÙ„Ø©.")
