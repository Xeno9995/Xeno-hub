local Tab4 = Window:MakeTab({"Player", "User"})
local Section = Tab4:AddSection({"Choose any player to know their information"})
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- المتغيرات
local selectedPlayer = nil
local espEnabled = false
local viewEnabled = false
local connections = {}

-- تحديث Dropdown تلقائيًا
local function GetPlayerNames()
    local names = {}
    for _, plr in ipairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer then
            table.insert(names, plr.Name)
        end
    end
    return names
end

-- مشاهدة اللاعب
local function SetView(enabled)
    if not selectedPlayer then return end
    viewEnabled = enabled
    if enabled then
        local function followCam()
            while viewEnabled and selectedPlayer and selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("Head") do
                workspace.CurrentCamera.CameraSubject = selectedPlayer.Character.Head
                task.wait()
            end
        end
        task.spawn(followCam)
    else
        workspace.CurrentCamera.CameraSubject = LocalPlayer.Character:FindFirstChild("Humanoid") or LocalPlayer.Character
    end
end

-- ESP
local function CreateESP(target)
    if not target.Character or not target.Character:FindFirstChild("Head") then return end

    local billboard = Instance.new("BillboardGui")
    billboard.Name = "TargetESP"
    billboard.Adornee = target.Character.Head
    billboard.Size = UDim2.new(0, 200, 0, 50)
    billboard.StudsOffset = Vector3.new(0, 3, 0)
    billboard.AlwaysOnTop = true

    local nameLabel = Instance.new("TextLabel", billboard)
    nameLabel.Size = UDim2.new(1, 0, 0.5, 0)
    nameLabel.BackgroundTransparency = 1
    nameLabel.Text = target.Name
    nameLabel.TextColor3 = Color3.new(1, 0, 0)
    nameLabel.TextStrokeTransparency = 0.5
    nameLabel.TextScaled = true

    local distLabel = Instance.new("TextLabel", billboard)
    distLabel.Position = UDim2.new(0, 0, 0.5, 0)
    distLabel.Size = UDim2.new(1, 0, 0.5, 0)
    distLabel.BackgroundTransparency = 1
    distLabel.TextColor3 = Color3.new(1, 1, 1)
    distLabel.TextStrokeTransparency = 0.5
    distLabel.TextScaled = true

    billboard.Parent = target.Character.Head

    local conn = game:GetService("RunService").RenderStepped:Connect(function()
        if target and target.Character and target.Character:FindFirstChild("Head") then
            local distance = math.floor((LocalPlayer.Character.Head.Position - target.Character.Head.Position).Magnitude)
            distLabel.Text = "Distance: " .. distance
        end
    end)

    table.insert(connections, conn)
end

local function RemoveESP()
    if selectedPlayer and selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("Head") then
        for _, v in pairs(selectedPlayer.Character.Head:GetChildren()) do
            if v:IsA("BillboardGui") and v.Name == "TargetESP" then
                v:Destroy()
            end
        end
    end
    for _, conn in ipairs(connections) do
        conn:Disconnect()
    end
    connections = {}
end

-- Teleport
local function TeleportToPlayer()
    if selectedPlayer and selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("Head") then
        LocalPlayer.Character:MoveTo(selectedPlayer.Character.Head.Position + Vector3.new(2, 0, 2))
    end
end

-- Dropdown
Tab4:AddDropdown({
    Name = "Select Player",
    Options = GetPlayerNames(),
    Default = "",
    Callback = function(selected)
        selectedPlayer = Players:FindFirstChild(selected)
        RemoveESP()
        if espEnabled and selectedPlayer then
            CreateESP(selectedPlayer)
        end
        if viewEnabled then
            SetView(true)
        end
    end
})

-- تحديث تلقائي إذا دخل أو خرج لاعب
Players.PlayerAdded:Connect(function()
    task.wait(1)
    -- يُفضّل ربط هذا مع إعادة تعيين خيارات Dropdown إذا كانت مكتبتك تدعم
end)

Players.PlayerRemoving:Connect(function(plr)
    if selectedPlayer == plr then
        selectedPlayer = nil
        RemoveESP()
        SetView(false)
    end
end)

-- View Toggle
Tab4:AddToggle({
    Name = "View",
    Default = false,
    Callback = function(V)
        SetView(V)
    end
})

-- ESP Toggle
Tab4:AddToggle({
    Name = "Esp",
    Default = false,
    Callback = function(V)
        espEnabled = V
        if V and selectedPlayer then
            CreateESP(selectedPlayer)
        else
            RemoveESP()
        end
    end
})

-- Teleport Toggle
Tab4:AddToggle({
    Name = "Teleport",
    Default = false,
    Callback = function(V)
        if V and selectedPlayer then
            TeleportToPlayer()
        end
    end
})
local Section = Tab4:AddSection({"Show Character Player With Animation"})
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer
local Camera = workspace.CurrentCamera

local selectedPlayer = nil
local orbitEnabled = false
local orbitConnection = nil
local orbitRadius = 10
local orbitSpeed = 1

-- تحديث قائمة اللاعبين
local function GetPlayerNames()
    local names = {}
    for _, plr in ipairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer then
            table.insert(names, plr.Name)
        end
    end
    return names
end

-- وظيفة الدوران
local function StartOrbit()
    if not selectedPlayer or not selectedPlayer.Character or not selectedPlayer.Character:FindFirstChild("Head") then return end

    local angle = 0
    orbitConnection = RunService.RenderStepped:Connect(function(dt)
        if not orbitEnabled or not selectedPlayer or not selectedPlayer.Character or not selectedPlayer.Character:FindFirstChild("Head") then return end
        angle = angle + orbitSpeed * dt
        local targetPos = selectedPlayer.Character.Head.Position
        local offset = Vector3.new(math.cos(angle), 0.3, math.sin(angle)) * orbitRadius
        local cameraPos = targetPos + offset
        Camera.CameraType = Enum.CameraType.Scriptable
        Camera.CFrame = CFrame.new(cameraPos, targetPos)
    end)
end

local function StopOrbit()
    if orbitConnection then
        orbitConnection:Disconnect()
        orbitConnection = nil
    end
    Camera.CameraType = Enum.CameraType.Custom
end

-- Dropdown لاختيار اللاعب
Tab4:AddDropdown({
    Name = "Select Player",
    Options = GetPlayerNames(),
    Default = "",
    Callback = function(selected)
        selectedPlayer = Players:FindFirstChild(selected)
        if orbitEnabled then
            StopOrbit()
            StartOrbit()
        end
    end
})

-- زر تفعيل الدوران
Tab4:AddToggle({
    Name = "Show Character",
    Default = false,
    Callback = function(state)
        orbitEnabled = state
        if state then
            StartOrbit()
        else
            StopOrbit()
        end
    end
})
local Section = Tab4:AddSection({"Walking through the walls"})
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local noclipConnection
local noclipEnabled = false

Tab4:AddToggle({
    Name = "Noclip",
    Default = false,
    Callback = function(state)
        noclipEnabled = state

        if noclipEnabled then
            noclipConnection = RunService.Stepped:Connect(function()
                if player.Character and player.Character:FindFirstChild("Humanoid") then
                    for _, part in ipairs(player.Character:GetDescendants()) do
                        if part:IsA("BasePart") and part.CanCollide == true then
                            part.CanCollide = false
                        end
                    end
                end
            end)
            print("🚀 Noclip Enabled")
        else
            if noclipConnection then
                noclipConnection:Disconnect()
                noclipConnection = nil
            end

            -- إعادة التفعيل للتصادم عند إيقاف Noclip
            if player.Character then
                for _, part in ipairs(player.Character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = true
                    end
                end
            end
            print("🛑 Noclip Disabled")
        end
    end
})
