loadstring(game:HttpGet("https://raw.githubusercontent.com/Xeno9995/XENOLIB/refs/heads/main/XENO%20HUB%20FLY%20GUI%20SCRIPT"))()
 print("Hello!")
 end)
local bringTab       = createTab("BRING", "XN")
createSection(bringTab, "Bring Everything In Map")
-- ŸÖÿ™ÿ∫Ÿäÿ± ŸÑÿ™ÿÆÿ≤ŸäŸÜ ÿßŸÑÿßÿÆÿ™Ÿäÿßÿ± ŸÖŸÜ ÿßŸÑŸÄ Dropdown
local selectedFoodItem = nil

-- Dropdown ŸÑŸÑÿßÿÆÿ™Ÿäÿßÿ± ÿ®ŸäŸÜ Steak Ÿà Morsel Ÿà All
createDropdown(bringTab, {"Steak", "Morsel", "All"}, 1, function(opt)
    selectedFoodItem = opt
end)

-- ÿØÿßŸÑÿ© ÿ™ÿ¨Ÿäÿ® ÿ¨ŸÖŸäÿπ ÿßŸÑÿ®ÿßÿ±ÿ™ÿßÿ™ ŸÖŸÜ ÿ£Ÿä ŸÖŸàÿØŸäŸÑ ÿ£Ÿà ŸÖÿ¨ŸÑÿØ
local function getAllParts(obj, parts)
    parts = parts or {}
    if obj:IsA("BasePart") then
        table.insert(parts, obj)
    else
        for _, child in pairs(obj:GetChildren()) do
            getAllParts(child, parts)
        end
    end
    return parts
end

-- ÿØÿßŸÑÿ© ÿ≥ÿ≠ÿ® ÿßŸÑÿ£ÿ∫ÿ±ÿßÿ∂ ÿ≠ŸàŸÑ ÿßŸÑŸÑÿßÿπÿ®
local function bringItems(filter)
    local player = game.Players.LocalPlayer
    local char = player.Character or player.CharacterAdded:Wait()
    local hrp = char:FindFirstChild("HumanoidRootPart")
    if not hrp then return end

    local radius = 6
    local angleStep = math.rad(25)
    local currentAngle = 0

    for _, obj in pairs(workspace:GetDescendants()) do
        local name = obj.Name:lower()
        if filter(name) then
            local parts = getAllParts(obj)
            for _, part in pairs(parts) do
                if part:IsA("BasePart") then
                    -- ÿ¨ÿπŸÑŸá ŸÇÿßÿ®ŸÑ ŸÑŸÑŸÖÿ≥ Ÿà ÿ∫Ÿäÿ± ŸÖÿ´ÿ®ÿ™
                    part.Anchored = false
                    part.CanCollide = true
                    if not part.Parent or part.Parent == nil then
                        part.Parent = workspace
                    end

                    -- Ÿàÿ≤ÿπŸá ÿ≠ŸàÿßŸÑŸäŸÜ ÿßŸÑŸÑÿßÿπÿ®
                    local x = math.cos(currentAngle) * radius
                    local z = math.sin(currentAngle) * radius
                    part.CFrame = hrp.CFrame * CFrame.new(x, 0, z)
                    currentAngle = currentAngle + angleStep
                end
            end
        end
    end
end

-- ÿ≤ÿ± ŸÑÿ¨ŸÑÿ® ÿßŸÑÿ£ŸÉŸÑ ÿßŸÑŸÖÿÆÿ™ÿßÿ±
createButton(bringTab, "Bring Food", function()
    if not selectedFoodItem then
        warn("ÿßŸÑÿ±ÿ¨ÿßÿ° ÿßÿÆÿ™Ÿäÿßÿ± ŸÜŸàÿπ ÿßŸÑÿ∑ÿπÿßŸÖ ŸÖŸÜ ÿßŸÑŸÄ Dropdown ÿ£ŸàŸÑÿßŸã!")
        return
    end

    if selectedFoodItem == "Morsel" then
        bringItems(function(name)
            return name:match("morsel")
        end)
    elseif selectedFoodItem == "Steak" then
        bringItems(function(name)
            return name:match("steak") or name:match("meat")
        end)
    elseif selectedFoodItem == "All" then
        bringItems(function(name)
            return name:match("morsel") or name:match("steak") or name:match("meat")
        end)
    end

    print("‚úÖ ÿ™ŸÖ ÿ•ÿ≠ÿ∂ÿßÿ±: " .. selectedFoodItem)
end)
-- ŸÖÿ™ÿ∫Ÿäÿ± ŸÑÿ™ÿÆÿ≤ŸäŸÜ ÿßŸÑÿßÿÆÿ™Ÿäÿßÿ± ŸÖŸÜ ÿßŸÑŸÄ Dropdown
